{
  "name"         : "fboseca/files_manager",
  "description"  : "Associate Laravel`s models with files uploaded",
  "type"         : "package",
  "license"      : "mit",
  "authors"      : [
    {
      "name"  : "Fboseca",
      "email" : "f.borjasercas@gmail.com"
    }
  ],
  "require"      : {
    "php"               : ">=7.1.3",
    "laravel/framework" : ">=6"
  },
  "extra"        : {
    "laravel" : {
      "providers" : [
        "Fboseca\\Filesmanager\\FilesManagerServiceProvider"
      ]
    }
  },
  "autoload"     : {
    "psr-4" : {
      "Fboseca\\Filesmanager\\" : "src"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Fboseca\\Filesmanager\\Tests\\" : "tests"
    }
  },
  "require-dev"  : {
    "orchestra/testbench" : "5.x-dev",
    "phpunit/phpunit"     : "8.5.x-dev",
    "orchestra/canvas"    : "5.x-dev"
  },
  "scripts"      : {
    "test"    : "php vendor/phpunit/phpunit/phpunit --testdox --colors=always --group",
    "testone" : "php vendor/phpunit/phpunit/phpunit  --testdox --colors=always --filter",
    "artisan" : "php vendor/orchestra/canvas/canvas"
  }
}
